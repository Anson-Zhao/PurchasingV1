        table.clear();
        table.draw();
        trapTable.clear();
        trapTable.draw();
        queryAndDisplay(queryStr);
    }

    var queryAndDisplay = function (querystr) {

        $.ajax({
            url: '/filterQuery',
            type: 'GET',
            dataType: 'json',
            async: false,
            data: querystr,
            success: function (resp) {

                if (!resp[0].errMsg) {
                    for (var i = 0; i < resp.length; i++) {
                        if (resp[i].Entry_type === "FIELD SCOUTING") {
                            table.row.add([
                                resp[i].transactionID,
                                resp[i].Date,
                                resp[i].Location_name,
                                resp[i].Latitude,
                                resp[i].Longitude,
                                resp[i].Main_crop,
                                resp[i].Main_crop_variety,
                                resp[i].Irrigation,
